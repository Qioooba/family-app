      "autoAudits": false,
      "showShadowRootInWkwebview": true,
      "uglifyFileName": false
    },
    "usingComponents": true,
    "permission": {
      "scope.userLocation": {
        "desc": "您的位置信息将用于小程序位置接口的效果展示"
      }
    }
  },
  "mp-alipay": {
    "usingComponents": true
